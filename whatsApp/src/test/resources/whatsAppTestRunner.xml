<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="WhatsApp Automation" parallel="false">
    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="mac"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="63.0"/>
    <parameter name="url" value="https://www.whatsapp.com/"/>

    <test name = "WhatsApp Automation ">
        <classes>
            <class name="testcases.TestMainPage"/>
            <class name="testcases.TestWhatAppWebLink"/>
        </classes>
    </test>

</suite>