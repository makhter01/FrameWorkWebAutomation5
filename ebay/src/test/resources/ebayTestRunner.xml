<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="ebay Automation Test" parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="Windows"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="63.0"/>
    <parameter name="url" value="https://www.ebay.com/"/>

    <test name="ebay Home Automation 1" preserve-order="true" >
    <classes>
            <class name="testcases.TestHomePage"/>
     </classes>
    </test>
    <test name="ebay Home Automation 2" preserve-order="true" >
    <classes>
            <class name="testcases.TestSearchItemPage"/>
    </classes>
    </test>
    <test name="ebay Home Automation 3" preserve-order="true" >
        <classes>
            <class name="testcases.TestSignIn"/>
        </classes>
    </test>
    <test name="ebay Home Automation 4" preserve-order="true" >
        <classes>
            <class name="testcases.TestRegistration"/>
        </classes>
    </test>
    <test name="ebay Home Automation 5" preserve-order="true" >
        <classes>
            <class name="testgooglesheet.Testgooglesheet"/>
        </classes>
    </test>
    <test name="ebay Home Automation 6" preserve-order="true" >
        <classes>
            <class name="testfeatures.TestFeatures"/>
        </classes>
    </test>
    <test name="ebay Home Automation 7" preserve-order="true" >
        <classes>
            <class name="testcases.TestInputExcelSheet"/>
        </classes>
    </test>

</suite>



